<div class="container col-md-6 col-md-offset-3">
	
	<h1 class="text center">Update Information</h1>
	
</div>
<div class="col-md-6 col-md-offset-3" >

		<form action="/edit/update" method="post" enctype="multipart/form-data">

			<div class="form-group">
				<label class="control-label">Fullname</label>
				<input type="text" name="fullname" class="form-control" value="<%=update.fullname%>">
			</div>


			<div class="form-group">
				<label class="control-label">Address</label>
				<textarea class="form-control" name="address"><%=update.address%></textarea>
			</div>

			<div class="form-group">
				<label class="control-label">Phone</label>
				<input type="text" name="phone" class="form-control" value="<%=update.phone%>">
			</div>

			<div class="form-group">
				<label class="control-label">Email</label>
				<input type="text" name="email" class="form-control" value="<%=update.email%>">
			</div>


			<div class="form-group">
				<label class="control-label">Username</label>
				<input type="text" name="username" class="form-control" value="<%=update.username%>">
			</div>
			

			<div class="form-group">
				<label class="control-label">Upload Image</label>
				<input type="file" name="image">
			</div>
			
			<br>
			<input type="submit" value="Update" class="btn btn-primary">
		</form>

		
		</div>